<h3>My projects with estimates</h3>
<% @projects.each do |project| %>
 <p><%= project.name %></p>
 <p><%= project.street_and_nr %></p>
<% end %>

<div
  id="map"
  style="width: 40%;
  height: 500px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>


<h1>My projects</h1>
<ul id=“projects”>
<% @projects.each do |project|%>
 <li><%= link_to project.name, project_path(project) %></li>
   <% end %>
</ul>









